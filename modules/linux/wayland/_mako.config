[app-name=sway]
max-icon-size=64
format=%b
width=425
height=200
font=JetBrainsMono Nerd Font Semibold 16
padding=12
default-timeout=2000

[app-name=sway body~=.*.*]
background-color=#48B810D0

[app-name=sway body~=.*.*]
background-color=#E04010D0
default-timeout=3000

